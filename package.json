{
  "name": "business",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/bin/www",
    "dev": "DEBUG=nkapsi:* NODE_ENV=development npm-run-all build server",
    "testing": "NODE_ENV=test npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch:dev": "nodemon",
    "test": "DEBUG=nkapsi:* jest --detectOpenHandles"
  },
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "cookie-parser": "~1.4.4",
    "debug": "^4.1.1",
    "eslint": "6.8.x",
    "eslint-config-airbnb-base": "14.1.x",
    "eslint-plugin-import": "2.20.x",
    "express": "~4.17.3",
    "faker": "^4.1.0",
    "jest": "^26.1.0",
    "kafka-node": "^5.0.0",
    "mongoose": "^5.9.22",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests___",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/",
      "/public/"
    ],
    "testPathIgnorePatterns": [
      "/src",
      "/public/",
      "dist/"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/register": "^7.10.4"
  }
}
